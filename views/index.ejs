<<!DOCTYPE html>
<html>
  <head>
   
    <title>A-Frame</title>
    <meta name="description" content="A-Frame">
    <script src="https://aframe.io/releases/0.7.0/aframe.min.js"></script>
  </head>
  <body>
    <a-scene>
      <a-box position = '-1 .5 -3' rotation = '0 45 0' color='#4CC3D9'></a-box>
      <a-box position = '0 2 -4' rotation = '0 60 0' color='#4CC3D9'></a-box>
      <a-box position = '-5 2 -4' rotation = '0 60 0' color='#4CC3D9'></a-box>
      <a-sphere position = '-5 2 -6' rotation = '0 60 0' color='#4CC3D9'>
        <a-ring position = '0 0 0' rotation = '0 60 0' color='#4CC3D9'></a-ring> >        </a-ring>
      </a-sphere>
      <a-entity rotation = "0 180 0">
        <a-cylinder position="1 0.75 -3" rotation="0 0 0" radius="0.5" height="1.5" color="#FFC65D"></a-cylinder>
        <a-plane position="0 0 -4" rotation="-90 0 0" width="4" height="4" color="#7BC8A4"></a-plane>
      </a-entity>

      <a-sky color="#ECECEC"></a-sky>

    
    </a-scene>

  </body>
</html>>